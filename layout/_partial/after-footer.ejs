<% if (theme.fancybox){ %>
  <%- css('//cdn.bootcss.com/fancybox/2.1.5/jquery.fancybox.min') %>
<% } %>

<%- css('//cdn.bootcss.com/font-awesome/4.5.0/css/font-awesome.min') %>

<script>
	var yiliaConfig = {
		fancybox: <%=theme.fancybox%>,
		mathjax: <%=theme.mathjax%>,
		animate: <%=theme.animate%>,
		isHome: <%=is_home()%>,
		isPost: <%=is_post()%>,
		isArchive: <%=is_archive()%>,
		isTag: <%=is_tag()%>,
		isCategory: <%=is_category()%>,
		open_in_new: <%=theme.open_in_new%>
	}
</script>

<%- js('//cdn.bootcss.com/require.js/2.1.22/require.min') %>
<%- js('//cdn.bootcss.com/jquery/2.2.1/jquery.min') %>
<%- js('//cdn.bootcss.com/jquery.lazyload/1.9.1/jquery.lazyload.min') %>
<%- js('//cdn.bootcss.com/fancybox/2.1.5/jquery.fancybox.pack') %>
<%- js('static/js/main') %>

<%- partial('google-analytics') %>

<% if (theme.mathjax){ %>
<%- partial('mathjax') %>
<% } %>